<Window x:Class="_422_Zhukov_Baranov.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_422_Zhukov_Baranov"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid Margin="0,0,0,-31">
        <TextBlock HorizontalAlignment="Left" Margin="88,146,0,0" TextWrapping="Wrap" Text="x" VerticalAlignment="Top"/>
        <TextBlock HorizontalAlignment="Left" Margin="86,189,0,0" TextWrapping="Wrap" Text="i" VerticalAlignment="Top"/>
        <TextBox x:Name ="xTextbox" HorizontalAlignment="Left" Margin="111,146,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="iTextbox" HorizontalAlignment="Left" Margin="111,187,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button Content="Вычислить" HorizontalAlignment="Left" Margin="320,179,0,0" VerticalAlignment="Top" Height="41" Width="80" Click="Calculate_Button_Click"/>
        <Button Content="Очистить" HorizontalAlignment="Left" Margin="320,248,0,0" VerticalAlignment="Top" Height="41" Width="80" Click ="Clear_Button_Click"/>
        <TextBlock HorizontalAlignment="Left" Margin="62,365,0,0" TextWrapping="Wrap" Text="Ответ" VerticalAlignment="Top" Width="45"/>
        <TextBox x:Name ="TextBox1" HorizontalAlignment="Left" Margin="112,365,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120" IsReadOnly="True" />
        <Image HorizontalAlignment="Left" Height="133" Margin="12,20,0,0" VerticalAlignment="Top" Width="320" Source="Image/image.png"/>
        <RadioButton x:Name ="rad_sh" Content="sh(x)" HorizontalAlignment="Left" Margin="91,248,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name ="rad_x2" Content="x^2" HorizontalAlignment="Left" Margin="91,277,0,0" VerticalAlignment="Top"/>
        <RadioButton x:Name ="rad_ex" Content="e^x" HorizontalAlignment="Left" Margin="91,304,0,0" VerticalAlignment="Top"/>
    </Grid>
</Window>